# Uncomment the next line to define a global platform for your project
# platform :ios, '14.0'

target 'App' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!
  pod 'WCDB.swift'
  # Pods for App
  target 'AppWidgetExtension' do
    use_frameworks!
    pod 'WCDB.swift'
    
    target 'AppWidgetIntentHandler' do
      use_frameworks!
      pod 'WCDB.swift'
    end
  end
  
  target 'AppTests' do
    use_frameworks!
    pod 'WCDB.swift'
  end
end


